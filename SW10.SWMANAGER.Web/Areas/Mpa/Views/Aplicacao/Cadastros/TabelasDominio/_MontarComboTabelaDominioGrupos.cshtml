@model SW10.SWMANAGER.Web.Areas.Mpa.Models.Aplicacao.Cadastros.TabelasDominio.MontarComboTabelaDominioGruposViewModel

@if(Model.GruposTipoTabelaDominio.Count() > 0)
{
    <div class="form-group form-md-line-input form-md-floating-label no-hint">
        @Html.DropDownListFor(m => m.GrupoTipoTabelaDominioId,Model.GruposTipoTabelaDominio,L("FiltroGrupo"),new { @class = "form-control chosen-select",data_live_search = "true",data_none_selected_text = L("FiltroGrupo"),id = "filtro-tabela-dominio-grupo-id" })
        <label>@L("GrupoTipoTabela")</label>
    </div>
}
