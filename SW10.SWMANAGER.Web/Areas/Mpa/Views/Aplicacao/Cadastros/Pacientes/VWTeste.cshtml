@using Abp.Web.Mvc.Extensions
@using SW10.SWMANAGER.Web.Navigation

@model SW10.SWMANAGER.Web.Areas.Mpa.Models.Aplicacao.Cadastros.Pacientes.VWTesteViewModel

@{
    ViewBag.CurrentPageName = PageNames.App.Pacientes.Paciente;
}

@section Styles
{
    @Html.IncludeStyle("~/Areas/Mpa/Views/Aplicacao/Cadastros/Pacientes/index.min.css")
    <link href="~/libs/jquery-chosen/css/chosen.css" rel="stylesheet" />
    <link href="~/libs/jquery-te/jquery-te-1.4.0.css" rel="stylesheet" />
}
@section Scripts
{
    @Html.IncludeScript("~/Areas/Mpa/Views/Aplicacao/Cadastros/Pacientes/VWTeste.js")
    @*@Html.IncludeScript("~/Areas/Mpa/Views/Aplicacao/Cadastros/VWTestes/_CriarOuEditarModal.js")*@
    <script src="~/libs/jquery-chosen/js/chosen.js"></script>
    <script src="~/libs/jquery-te/jquery-te-1.4.0.min.js"></script>
}
@* Teste Branch *@
<div class="row margin-bottom-5">

    <div class="col-xs-6">
        <div class="page-head">
            <div class="page-title">
                <h1>
                    <span>@L("VWTeste")</span>
                </h1>
            </div>
        </div>
    </div>

    <div class="col-xs-6 text-right">
        @*<button id="ExportarVWTestesParaExcelButton" class="btn btn-default"><i class="far fa-file-excel"></i> @L("ExportToExcel")</button>*@
        @*@if (IsGranted(AppPermissions.Pages_Tenant_Cadastros_CadastrosGlobais_Paciente_Create))
            {
            <button id="CreateNewVWTesteButton" class="btn btn-primary blue"><i class="fa fa-plus"></i> @L("CreateNewVWTeste")</button>
        }*@
    </div>
</div>
<div class="portlet light margin-bottom-0">
    <div class="portlet-body">
        <div class="form">
            <form id="VWTestesFilterForm" class="horizontal-form">
                <div class="form-body" enter-key="vm.getVWTestes()">
                    <div class="row margin-bottom-10">
                        <div class="col-md-12">
                            <div class="inputs inputs-full-width">
                                <div class="portlet-input">
                                    <form>
                                        <div class="input-group">
                                            <input id="VWTestesTableFilter" class="form-control" placeholder="@L("SearchWithThreeDot")" type="text" value="@Model.Filtro">
                                            <span class="input-group-btn">
                                                <button id="GetVWTestesButton" class="btn default" type="submit"><i class="icon-magnifier"></i></button>
                                            </span>
                                        </div>
                                    </form>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </form>
        </div>
        <div id="VWTestesTable"></div>
    </div>
</div>
