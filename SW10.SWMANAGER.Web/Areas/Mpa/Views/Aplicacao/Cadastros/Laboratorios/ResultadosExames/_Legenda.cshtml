@using SW10.SWMANAGER.ClassesAplicacao.Services.Cadastros.Laboratorios.Exames.Dto;

@model IEnumerable<ExameStatusDto>

@if (Model.Count() > 0)
{
    <table class="table table-responsive table-bordered table-condensed">
        <tr>
            @foreach (var item in Model)
            {
                <td style="width:3px;text-align:center;"><span class="sw-btn-display" style="background-color:@(item.Cor)"></span></td>
                <td style="width:20px;">@item.Descricao</td>
            }
        </tr>
    </table>
}