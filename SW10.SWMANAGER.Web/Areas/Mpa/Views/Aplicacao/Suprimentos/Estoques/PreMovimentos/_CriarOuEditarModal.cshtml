<style type="text/css">
    .loader {
        /*position: fixed;
        left: 0px;
        top: 20%;
        /*width: 100%;
        height: 100%;
        z-index: 9999;*/
        text-align: center;
    }


        .loader .loading {
            font-size: 22px;
            margin-top: -60px !important;
            margin-left: 30px;
        }

            .loader .loading span {
                font-size: 50px;
                animation-name: blink;
                animation-duration: 1.4s;
                animation-iteration-count: infinite;
                animation-fill-mode: both;
            }

                .loader .loading span:nth-child(2) {
                    animation-delay: .2s;
                }

                .loader .loading span:nth-child(3) {
                    animation-delay: .4s;
                }

        .loader .modal {
        }

        .loader .vertical-alignment-helper {
            display: table;
            height: 100%;
            width: 100%;
        }

        .loader .vertical-align-center {
            /* To center vertically */
            display: table-cell;
            vertical-align: middle;
        }

        .loader .modal-content {
            /* Bootstrap sets the size of the modal in the modal-dialog class, we need to inherit it */
            width: inherit;
            height: inherit;
            /* To center horizontally */
            margin: 0 auto;
        }

    .select2-container.select2-container--bootstrap.select2-container--open {
        z-index: 999999;
    }

    .portlet-title {
        margin: -6px -10px 10px !important;
    }
</style>

