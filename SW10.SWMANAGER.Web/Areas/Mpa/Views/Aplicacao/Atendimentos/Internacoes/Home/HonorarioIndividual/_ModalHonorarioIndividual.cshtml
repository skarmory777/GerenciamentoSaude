@using SW10.SWMANAGER.Web.Areas.Mpa.Models.Common.Modals

@model SW10.SWMANAGER.Web.Areas.Mpa.Models.Aplicacao.Atendimentos.Internacao.ModalHonorarioIndividualViewModel

@Html.Partial("~/Areas/Mpa/Views/Common/Modals/_ModalHeader.cshtml", new ModalHeaderViewModel(L("SolicitacaoInternacao")))

<script src="~/libs/PDFObject/pdfobject.js"></script>

<div class="modal-body container-fluid">
    <div class="row">
        <div class="col-md-12">
            <div id="HonorarioIndividual" style="margin:auto; width:90%; height:1000px; border:6px solid #6da6c6; border-radius: 5px;"></div>
        </div>
    </div>
</div>

<script>
    var caminho = "/Mpa/AtendimentoRelatorio/HonorarioIndividual?atendimentoId=" + '@Model.AtendimentoId';
    PDFObject.embed(caminho, "#HonorarioIndividual");
</script>

