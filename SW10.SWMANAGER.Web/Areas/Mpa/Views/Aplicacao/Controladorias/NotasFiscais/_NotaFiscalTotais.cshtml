                </div>
            </div>
            <div class="col-sm-3">
                <div class="form-group form-md-line-input form-md-floating-label no-hint">
                    <input type="text" name="vOutro" class="form-control@(icms.vOutro.ToString().IsNullOrEmpty() ? "" : " edited")" value="@Math.Round(icms.vOutro,2)" disabled>
                    <label>@L("OutrasDespesas")</label>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-sm-4">
                <div class="form-group form-md-line-input form-md-floating-label no-hint">
                    <input type="text" name="vProd" class="form-control@(icms.vProd.ToString().IsNullOrEmpty() ? "" : " edited")" value="@Math.Round(icms.vProd,2)" disabled>
