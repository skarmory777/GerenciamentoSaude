                </div>
            </div>
            <div class="col-sm-3">
                <div class="form-group form-md-line-input form-md-floating-label no-hint">
                    <input type="text" name="vISS" class="form-control@(iss.vISS.ToString().IsNullOrEmpty() ? "" : " edited")" value="@(iss.vISS.HasValue?Math.Round((double)iss.vISS,2):0)" disabled>
                    <label>@L("ValorIss")</label>
                </div>
            </div>
        </div>

