@using Abp.Extensions


@{
    var model = TempData["NotaFiscalTotal"] as NFe.Classes.Informacoes.Total.total; // SW10.SWMANAGER.ClassesAplicacao.Controladorias.NotasFiscais.NotaFiscalTotal;

}

