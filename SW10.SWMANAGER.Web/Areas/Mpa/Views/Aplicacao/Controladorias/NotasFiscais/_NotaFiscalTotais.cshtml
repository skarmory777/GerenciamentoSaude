                    <input type="text" name="vBC" class="form-control@(icms.vBC.ToString().IsNullOrEmpty() ? "" : " edited")" value="@Math.Round(icms.vBC,2)" disabled>
                    <label>@L("BaseCalculoIcms")</label>
                </div>
            </div>
            <div class="col-sm-3">
                <div class="form-group form-md-line-input form-md-floating-label no-hint">
                    <input type="text" name="vICMS" class="form-control@(icms.vICMS.ToString().IsNullOrEmpty() ? "" : " edited")" value="@Math.Round(icms.vICMS,2)" disabled>
                    <label>@L("ValorIcms")</label>
                </div>
            </div>
            <div class="col-sm-3">
                <div class="form-group form-md-line-input form-md-floating-label no-hint">
                    <input type="text" name="vCOFINS" class="form-control@(icms.vCOFINS.ToString().IsNullOrEmpty() ? "" : " edited")" value="@Math.Round(icms.vCOFINS,2)" disabled>
                    <label>@L("ValorCofins")</label>
