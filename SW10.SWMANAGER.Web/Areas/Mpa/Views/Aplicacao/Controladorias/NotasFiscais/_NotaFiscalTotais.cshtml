                    <input type="text" name="vBCST" class="form-control@(icms.vBCST.ToString().IsNullOrEmpty() ? "" : " edited")" value="@Math.Round(icms.vBCST,2)" disabled>
                    <label>@L("BaseCalculoIcmsSt")</label>
                </div>
            </div>
            <div class="col-sm-3">
                <div class="form-group form-md-line-input form-md-floating-label no-hint">
                    <input type="text" name="vDesc" class="form-control@(icms.vDesc.ToString().IsNullOrEmpty() ? "" : " edited")" value="@Math.Round(icms.vDesc,2)" disabled>
