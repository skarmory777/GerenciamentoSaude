    if (model.retTrib != null)
    {
        var ret = model.retTrib;
        <div class="row">
            <div class="col-sm-4">
                <div class="form-group form-md-line-input form-md-floating-label no-hint">
                    <input type="text" name="vBCIRRF" class="form-control@(ret.vBCIRRF.ToString().IsNullOrEmpty() ? "" : " edited")" value="@(ret.vBCIRRF.HasValue?Math.Round((double)ret.vBCIRRF,2):0)" disabled>
                    <label>@L("BaseCalculoIrrf")</label>
                </div>
            </div>
            <div class="col-sm-4">
                <div class="form-group form-md-line-input form-md-floating-label no-hint">
                    <input type="text" name="vBCRetPrev" class="form-control@(ret.vBCRetPrev.ToString().IsNullOrEmpty() ? "" : " edited")" value="@(ret.vBCRetPrev.HasValue?Math.Round((double)ret.vBCRetPrev,2):0)" disabled>
                    <label>@L("BaseCalculoRetencao")</label>
                </div>
            </div>
            <div class="col-sm-4">
                <div class="form-group form-md-line-input form-md-floating-label no-hint">
