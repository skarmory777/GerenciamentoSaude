                    <input type="text" name="vNF" class="form-control@(icms.vNF.ToString().IsNullOrEmpty() ? "" : " edited")" value="@Math.Round(icms.vNF,2)" disabled>
                    <label>@L("ValorNotaFiscal")</label>
                </div>
            </div>
            <div class="col-sm-4">
                <div class="form-group form-md-line-input form-md-floating-label no-hint">
                    <input type="text" name="vTotTrib" class="form-control@(icms.vTotTrib.ToString().IsNullOrEmpty() ? "" : " edited")" value="@Math.Round(icms.vTotTrib,2)" disabled>
                    <label>@L("TotalTributo")</label>
                </div>
            </div>
        </div>
    }
    if (model.ISSQNtot != null)
    {
        var iss = model.ISSQNtot;
        <div class="row">
            <div class="col-sm-3">
